<ab><lb n="1"/>  <persName type="aphrodisian"><name><supplied reason="lost">Εὐ</supplied>πρέπης</name></persName>  <space extent="1" unit="character" dim="horizontal"/>  <persName type="divine" full="yes"><w lemma="θεός">θε<supplied reason="lost">ῷ</supplied></w><lb n="2"/><w lemma="ἐπήκοος"><supplied reason="lost" cert="low">ἐπηκόῳ</supplied></w></persName>  <w lemma="εὐχή">εὐχή<supplied reason="lost">ν</supplied></w></ab>
