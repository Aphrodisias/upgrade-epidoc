<ab><lb n="1"/><persName type="aphrodisian"><name><gap reason="lost" extent="unknown" unit="character" dim="left"/>ωνία</name></persName>  <gap reason="lost" extent="unknown" unit="character" dim="right"/><lb n="2"/><name><gap reason="lost" extent="unknown" unit="character" dim="left"/><supplied reason="lost">ων</supplied>ίου</name>  <w>θυ<supplied reason="lost">γατὴρ</supplied></w>  <gap reason="lost" extent="unknown" unit="character" dim="right"/><lb n="3"/><gap reason="lost" extent="unknown" unit="character" dim="left"/><orig n="unresolved"><unclear reason="damage">ινλυ</unclear></orig><gap reason="lost" extent="unknown" unit="character" dim="right"/><lb n="3a"/><gap reason="lost" unit="line" dim="bottom" extent="unknown"/></ab>
