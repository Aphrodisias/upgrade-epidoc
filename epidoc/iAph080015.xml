<ab>
<lb n="1"/>
<w lemma="τόπος">τόπο<unclear reason="damage">ς</unclear></w> <w lemma="ἀρχεόλογος"><supplied reason="lost">ἀρ</supplied><unclear reason="damage">χ</unclear>εολόγων</w></ab>
