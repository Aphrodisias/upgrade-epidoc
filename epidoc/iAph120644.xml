<ab><lb n="0"/><supplied reason="lost"><rs type="subject" key="CouncilPeople"><w lemma="ὁ">Ἡ</w> <w lemma="βουλή">βουλὴ</w> <w lemma="καί">καὶ</w> <w lemma="ὁ">ὁ</w></rs></supplied><lb n="1"/><g ref="#scroll"/> <w lemma="δῆμος">δῆμος</w> <g ref="#scroll"/><lb n="2"/><persName key="AurDiogenes" type="attested"><name nymRef="#Μᾶρκος">Μᾶρκον</name> <name nymRef="#Αὐρήλιος">Αὐρήλιον</name><lb n="3"/><name nymRef="#Διογένης">Διογέ<hi rend="ligature">νην</hi></name></persName> <rs key="legaug" type="official-secul"><w lemma="πρεσβευτής">πρεσ<lb n="4" break="no"/>βευτ<hi rend="ligature">ὴν</hi></w> <rs key="augustus" type="dignity"><w lemma="σεβαστός">Σεβασ<lb n="5" break="no"/>τῶν</w></rs> <w lemma="ἀντιστράτηγος">ἀντιστρά<lb n="6" break="no"/><hi rend="ligature">τη</hi>γον</w></rs> <g ref="#stop"/> <w lemma="ὁ">τὸν</w> <w lemma="δίκαιος">δίκαι<lb n="7" break="no"/>ον</w> <w lemma="καί">καὶ</w> <w lemma="ἁγνός">ἁγνὸν</w><lb n="8"/><w lemma="καί">καὶ</w> <w lemma="ἀνδρεῖος">ἀνδρεῖον</w><lb n="9"/><w lemma="καί">καὶ</w> <w lemma="πᾶς">πά<unclear reason="damage">σ</unclear>ῃ</w> <w lemma="ἀρετή">ἀρε<lb n="10" break="no"/>τῇ</w> <w lemma="κοσμέω">κεκοσμη<lb n="11" break="no"/><g ref="#scroll"/>μένον</w> <g ref="#scroll"/><lb n="11a"/><space extent="1" dim="vertical" unit="line"/><lb n="12"/><w lemma="προνοέομαι">προ<hi rend="ligature" id="hi4">ν</hi><supplied reason="omitted">ο</supplied><hi rend="ligature" id="hi5">η</hi>σαμένου</w><link targets="hi4 hi5" targOrder="U"/> <w lemma="ὁ"><hi rend="ligature">τῆ</hi>ς</w><lb n="13"/><w lemma="ἀνάστασις">ἀναστάσεως</w> <persName key="Nicomachus1" type="attested"><name nymRef="#Ἀντώνιος">Ἀντω<lb n="14" break="no"/>νίου</name> <name nymRef="#Νεικόμαχος">Νεικομάχου</name></persName><lb n="15"/><w lemma="ὁ">τοῦ</w> <w lemma="πατήρ">πατρὸς</w> <w lemma="ὁ">τοῦ</w> <rs key="archon" type="official-secul"><w lemma="πρῶτος">πρώ<lb n="16" break="no"/>του</w> <w lemma="ἄρχων">ἄρχοντος</w></rs> <persName key="Nicomachus2" type="attested"><name