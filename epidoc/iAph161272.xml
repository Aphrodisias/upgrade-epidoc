<ab><lb n="1"/>  <space extent="3" unit="character"/>  <persName type="divine"><supplied reason="lost">Ἀφροδί</supplied>τηι</persName>  <persName type="divine"><w>θεοῖς</w>  <persName type="emperor">Σ<supplied reason="lost">εβ</supplied>α<supplied reason="lost">σ</supplied><milestone unit="block" n="b"/><supplied reason="lost">τ</supplied>οῖς</persName></persName>  <g ref="#stop"/> <w lemma="ὁ">τῶι</w> <w>Δήμωι</w> <g ref="#stop"/> <w lemma="ὁ">τὸ</w> <w>πρόπυλον</w> <g ref="#stop"/>  <w lemma="καί">κα<unclear reason="damage">ὶ</unclear></w>  <milestone unit="block" n="c"/> <w>τὰς</w> <w lemma="ἐν">ἐν</w> <w>αὐτῷ</w> <w>τιμὰ<unclear reason="damage">ς</unclear></w>  <gap reason="lost" unit="character" extent="unknown"/>    <supplied reason="lost" cert="low"><space extent="3" unit="character"/></supplied><lb n="2"/><space extent="3" unit="character"/>    <persName nymRef="#Εὐσεβὴς  Μενάνδρου  Εὐνίκου" type="attested"><name nymRef="#Εὐσεβής">Εὐσεβὴ<supplied reason="lost">ς</supplied></name>  <w><supplied reason="lost">φιλόπ</supplied>ατρις</w></persName> <w lemma="καί">καὶ</w> <persName nymRef="#Μένανδρος  Μενάνδρου  Εὐνίκου" type="attested"><name nymRef="#Μένανδρος">Μένα<milestone unit="block" n="b"/>νδρος</name> <w lemma="ὁ">οἱ</w> <persName type="attested"><name nymRef="#Μένανδρος">Μενάνδρου</name> <w lemma="ὁ">τοῦ</w> <persName type="attested"><name nymRef="#Εὔνικος">Εὐνίκου</name></persName></persName></persName>  <milestone unit="block" n="c"/> <w lemma="καί">καὶ</w> <persName type="attested"><supplied reason="lost"><name nymRef="#Ἀπφία">Ἀπφίας</name></supplied>  <persName type="attested"><name nymRef="#Μένανδρος"><supplied reason="lost">Μ</supplied><unclear reason="damage">ε</unclear>νάνδρου</name></persName>  <w>γ<supplied reason="lost">υν</supplied>ὴ</w>  <persName type="attested"><name nymRef="#Εὐσεβής">Εὐσεβοῦς</name></persName></persName></ab>
