<ab><lb n="1"/><gap reason="lost" unit="character" extent="unknown"/><orig n="unresolved"><unclear reason="damage">σ</unclear></orig>  <persName type="attested"><name nymRef="#Ἰάσων">Ἰασων</name></persName>  <w lemma="ὁ">τῷ</w>  <w>Δήμῳ</w>  <gap reason="lost" unit="character" extent="unknown"/></ab>
